@model ICollection<NetMeeting.Models.ExternalLogin>

@if (Model.Count > 0) {
  <h2>Registered external logins</h2>

  foreach (NetMeeting.Models.ExternalLogin externalLogin in Model) {
    if (ViewBag.ShowRemoveButton) {
      using (Html.BeginForm("Disassociate", "Account", FormMethod.Post, new { @class = "form-inline" })) {
  @Html.AntiForgeryToken()
  <fieldset>
    @Html.Hidden("provider", externalLogin.Provider)
    @Html.Hidden("providerUserId", externalLogin.ProviderUserId)
    <span>@externalLogin.ProviderDisplayName</span>
    <button type="submit" class="btn btn-danger" title="Remove this @externalLogin.ProviderDisplayName credential from your account">Remove</button>
  </fieldset>
      }
    }
  }
}